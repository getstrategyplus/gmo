  transition-delay: .1s;
  transform: translateY(0) scale(1);
}
.showabout .pagecontent {
  -webkit-filter: blur(6px);
 filter: blur(6px);
}
.about {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color:rgba(48,82,116,0.65);
  text-align: center;
  z-index: 60;
  color: #fff;
  transition: all 800ms cubic-bezier(0.230, 1.000, 0.320, 1.000);
  transform: translateY(50px) scale(1.1);
  opacity: 0;
  visibility: hidden;
}
.about .btn-close {
  position: absolute;
  right: 16px;
  top: 16px;
  z-index: 10;
}
.about .inner {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow-y: auto;
  transform: translate(0,0);
  padding-bottom: 25px;
}
.about .about-content {
  max-width: 750px;
