/* News */
.newsblock {
  padding-top: 12px;
}
.newsblock .intro {
  padding-left: 15px;
  padding-bottom: 20px;
  max-width: 600px;
  text-align: center;
  margin: 0 auto;
}
.newsblock .intro h5 {
  font-size: 1.2em;
  font-family:Georgia, Times New Roman, serif;
}

.list-news {
  margin: 0 15px;
}


.list-news > li {
  display: table;
  width: 100%;
  background-color: #fff;
  margin-bottom: 6px;
  position: relative;
  padding: 20px;
  padding-right: 50px;
  transition: .2s all;
}
.list-news > li.removenewsitem {
  transform: translateX(-25px);
  opacity: 0;
  visibility: hidden;
}
.list-news > li.addnewsitem {
  transform: translateX(25px);
  opacity: 0;
  visibility: hidden;
}
.list-news > li:after {
  content: "";
  display: table;
  clear: both;
}
.list-news > li > * {
  display: table-cell;
  vertical-align: middle;
}
.list-news > li:hover {
  box-shadow: 0 1px 6px rgba(0,0,0,.1);
}
.list-news > li:hover .news-actions {
  right: 0;
  visibility: visible;
  opacity: 1;
}
.list-news > li:hover .imgbox {
  transform: translateX(-5px);
}
.list-news .imgbox {
  width: 116px;
  margin-left: -25px;
  float: left;
  margin-right: 25px;
  transition: .3s ease-out;
  overflow: hidden;
}
.list-news a {
  color: rgba(0,0,0,.75);
}
.list-news .news-source {
  font-size: 11px;
  background-color: rgba(0,0,0,.05);
  padding: 2px 6px;
  margin-right: 6px;
  font-family: Arial, Helvetica, sans-serif;
  position: relative;
  top: -1px;
}
.list-news .news-source:hover {
  background-color: rgba(0,0,0,.45);
  color: #fff;
}
.list-news p {
  margin: 0;
}
.list-news .title {
  color: #0C4C8C;
  font-family: Volkorn, Times New Roman, serif;
  font-size: 26px;
  margin-bottom: 10px;
  line-height: 1.05em;
}
.list-news .short {
  font-size: 14px;
  color: rgba(0,0,0,.75);
}
.list-news .news-actions {
  transition: .2s all;
  position: absolute;
  right: -10px;
  top: 50%;
  transform: translateY(-50%);
  text-align: right;
  transition: .3s ease;
  opacity: 0;
  visibility: hidden;
  background-color: #0C4C8C;
}
.list-news .news-actions a {
  display: block;
  padding: 8px 10px;
  font-size: 13px;
  color:rgba(65,77,101,0.35);
  color: #fff;
}
